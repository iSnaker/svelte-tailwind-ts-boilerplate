{
  "name": "svelte-tailwind-ts-boilerplate",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "author": "Snaker",
  "scripts": {
    "dev": "vite --host",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "tailwind": "npx tailwindcss -i ./src/app.css -o ./src/theme.css --watch",
    "build-tailwind": "NODE_ENV=production npx tailwindcss -i ./src/app.css -o ./src/theme.css"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^1.0.1",
    "@tsconfig/svelte": "^3.0.0",
    "svelte": "^3.49.0",
    "svelte-check": "^2.8.0",
    "svelte-preprocess": "^4.10.7",
    "tailwindcss": "^3.1.6",
    "tslib": "^2.4.0",
    "typescript": "^4.6.4",
    "vite": "^3.0.0"
  },
  "dependencies": {
    "autoprefixer": "^10.4.7",
    "moment": "^2.29.4",
    "postcss": "^8.4.14",
    "svelte-flatpickr": "^3.2.6",
    "svelte-navigator": "^3.1.6"
  }
}
